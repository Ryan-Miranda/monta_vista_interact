@font-face 
{
    font-family:'caviar_dreamsregular';
    src: url('<%= asset_path('CaviarDreams-webfont.eot') %>');
    src: url('<%= asset_path('CaviarDreams-webfont.eot?#iefix') %>') format('embedded-opentype'),
          url('<%= asset_path('CaviarDreams-webfont.svg#caviar_dreamsregular') %>') format('svg'),
         url('<%= asset_path('CaviarDreams-webfont.woff') %>') format('woff'),
         url('<%= asset_path('CaviarDreams-webfont.ttf') %>') format('truetype');
    font-weight: normal;
    font-style: normal;
}
@font-face 
{
    font-family:'caviar_dreamsbold';
    src: url('<%= asset_path('Caviar_Dreams_Bold-webfont.eot') %>');
    src: url('<%= asset_path('Caviar_Dreams_Bold-webfont.eot?#iefix') %>') format('embedded-opentype'),
          url('<%= asset_path('Caviar_Dreams_Bold-webfont.svg#caviar_dreamsbold') %>') format('svg'),
         url('<%= asset_path('Caviar_Dreams_Bold-webfont.woff') %>') format('woff'),
         url('<%= asset_path('Caviar_Dreams_Bold-webfont.ttf') %>') format('truetype');
    font-weight: normal;
    font-style: normal;
}
#hide
{
  visibility: hidden;
}
#background-shade
{
  background:rgba(0,0,0,0.45);
  padding-bottom:0px;
}
#log-in
{
  position:relative;
  top:-20px;
}
#signed_in_msg
{
  position: absolute;
  top: 10%;
  left: 6%;
  text-align: right;
  color:#FFFFFF;
}
#links
{
  color: #FFFFFF;
  position:absolute;
  top:10%;
  left:6%;
}
#block_center
{
  margin-left: auto;
  margin-right: auto;
  width: 6em;
}
.footer {
    width: 100%;
    height: auto;
    background: rgba(38, 125, 241, 0.75);
    text-align: center;  
}
.top-right
{
  margin:0;
}
.m
{
  font-size:20px;
  color:#FFFFFF;
}
.links-bar
{
  position:absolute;
  top:60%;
  left:6%;
}
input
{
  font-size:40px;
}
.logo
{
  margin-top:-13px;
  width:480px;
  height:116px;
}
.navbar-header
{
  height:120px;
}
.header
{
  font-family: Merlin;
}
.navbar.navbar-default
{
  border:none;
  background:transparent;
  margin-bottom: 0px;
  width: 100%;
}
.btn.btn-primary:hover
{
    border:1px;
    margin:1px;
    text-decoration: none;
}
.text_center
{
  text-align:center;
}
.jumbotron
{
  background:transparent;
}
.jumbotron-text, h1.jumbotron-text
{
  color:#FFFFFF;
}
div .jumbotron
{
  margin-bottom:0;
  padding:0;
}
div .container
{
  position:relative;
  top:-30px;
}
.underlined
{
  color: #FFFFFF;
  font-size: 20px;
  border-bottom: 3px solid #CBE86B;
  text-decoration: none;
  opacity:1!important;
}
.inline
{
  color:#FFFFFF;
  margin-left:0px;
  display:inline;
}
.jumbotron .submit
{
  font-size:18px;
  width:100px;
  height:35px;
}
nav 
{
  display: block;
  text-align: right;
}
.jumbotron h1
{
  font-size:36px;
}
.jumbotron h2
{
  font-size:28px;
}
.jumbotron label, input, div
{
  font-size:16px;
}
.event_list
{
  font-size:20px;
  display:inline;
}
.text-muted
{
  bottom: 0;
  border:0;
  margin:0;
  color:#FFFFFF;
  font-size:16px;
}
h1,h2,h3,h4,h5
{
  font-family:'caviar_dreamsbold', Arial, sans-serif;
  color: #FFFFFF;
  text-rendering: optimizeLegibility;
}
h6
{
  font-family:'caviar_dreamsbold', Arial, sans-serif;
  color: #000000;
  text-rendering: optimizeLegibility;
}
button
{
  color:#000000;
}
input[type=submit]
{
  width: auto;
}
body
{
  position:relative;
  margin:0;
  margin:auto;
  font-family:'caviar_dreamsregular', Arial, sans-serif;
  font-size:100%;
  text-rendering: optimizeLegibility;
  background-image: url('https://www.kitchell.com/wp-content/uploads/2016/09/img_4550_color-edit-1024x460.jpg');
  background-repeat: no-repeat;
  background-size: cover;
  opacity:1.0;
  overflow-x:hidden;
  overflow-y:auto;
  max-width: 1366px;
  color:#FFFFFF;
}
form input[type=text], input[type=password]
{
width: 18%;
}
input
{
  color:#000000;
}
span
{
  font-size: 20px;
}
/*
css below is from http://cozar.me/ (cozar14.css-2014)
*/
a
{
  color: #FFFFFF;
}
a:hover, a:focus
{
  color: #FFFFFF;
  border-bottom: 2px solid #CBE86B;
  text-decoration: none!important;
  border-bottom-color: #CBE86B;
}
nav a,
#submit.active
{
  color: #FFFFFF;
  font-size: 16px;
  display: inline-block;
  margin-left: 25px;
}
nav a:hover,
#submit.active:hover,
.list-pro a:hover,
nav a.active,
.list-pro a.active,
.stButton .chicklets:hover,
a.active
{
  color: #FFFFFF;
  border-bottom: 3px solid #CBE86B;
  text-decoration: none;
  opacity:1!important;
}
